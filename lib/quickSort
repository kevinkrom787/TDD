let num = [7, 4, 6, 2, 5];

function quickSort(num) {
  if (num < 2) {
    return;
  }

  let greaterThan = [];
  let lessThan = [];
  let pivot = num.pop();


  num.forEach(num => {
    let pivot = num.pop();

    if (pivot > num) {
      greaterThan.push(number);
    }
    if (pivot < number) {
      lessThan.push(number);
    }

    quickSort(...lessThan, ...pivot, ...greaterThan);
    console.log(number);
  });
}
quickSort(num);